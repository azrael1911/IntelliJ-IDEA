<templateSet group="TemplatesGroup">
  <template name="*" value="*&#10; * @Description: $description$&#10; * @method: $method$&#10;$params$&#10; * @return: $return$&#10; * &#10; * @Creator: $creator$&#10; * @Date: $CreateDate$ $CreateTime$&#10; *&#10; * @Modify: $modify$&#10; * @Date: $ModifyDate$ $ModifyTime$ &#10; * */" description="方法注释" toReformat="true" toShortenFQNames="true">
    <variable name="description" expression="groovyScript(&quot;def result='请在此处输入方法描述信息';return result&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="method" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="params" expression="groovyScript(&quot;def result=''; def params=\&quot;${_1}\&quot;.replaceAll('[\\\\[|\\\\]|\\\\s]', '').split(',').toList(); for(i = 0; i &lt; params.size(); i++) {result+='* @param ' + params[i] + ((i &lt; params.size() - 1) ? '\\n' : '')}; return result&quot;, methodParameters())" defaultValue="" alwaysStopAt="false" />
    <variable name="return" expression="methodReturnType()" defaultValue="" alwaysStopAt="false" />
    <variable name="creator" expression="user()" defaultValue="" alwaysStopAt="false" />
    <variable name="CreateDate" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="CreateTime" expression="time()" defaultValue="" alwaysStopAt="false" />
    <variable name="modify" expression="user()" defaultValue="" alwaysStopAt="false" />
    <variable name="ModifyDate" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="ModifyTime" expression="time()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>